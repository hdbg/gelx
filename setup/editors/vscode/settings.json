{
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 2,
	"dprint.path": ".devenv/profile/bin/dprint",
	"rust-analyzer.cargo.features": "all",
	"rust-analyzer.check.command": "clippy",
	"[json]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[markdown]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[nix]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[rust]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[toml]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[yaml]": {
		"editor.defaultFormatter": "dprint.dprint"
	}
}
